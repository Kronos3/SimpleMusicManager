<div id="content-wrapper">
    <div class="albums">
        {{#albums}}
        <paper-card class="hoverable" id="album-{{index}}" image="{{img}}" onclick="window.APP.ui.goto_album(window.APP.ui.rawalbums.albums[{{index}}])">
            <div class="card-content">
                <p class="album-name truncate">{{name}}</p>
                <p class="album-artist truncate">{{artist}}</p>
            </div>
        </paper-card>
        {{/albums}}
    </div>
</div>
