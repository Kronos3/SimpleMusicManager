<div class="content-wrapper-artists">
    {{#artists}}
    <div class="artists" onclick="window.APP.ui.goto_artist(window.APP.ui.rawArtists.artists[{{index}}])">
        <img src="{{img}}" class="artists-img card">
        <p class="artists-name">{{name}}</p>
        <span class="artist-letter">{{letter}}</span>
    </div>
    {{/artists}}
</div>
